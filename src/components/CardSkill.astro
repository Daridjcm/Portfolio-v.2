---
interface Props {
    icons : string;
}

const { icons } = Astro.props;
---

<div class="flex flex-col pb-5">
    <div class="bg-[#23262D] p-5 border border-gray-700 flex-row mt-2 m-auto rounded-lg">
        <a href="https://skillicons.dev" class="block">
            <img class="max-w-full h-auto mx-auto" src={icons} alt="Skills">
        </a>
    </div>
</div>
